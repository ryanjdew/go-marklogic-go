{
  "load_variables_from": [
    {
      "variable_loader_type": "dotenv",
      "env_file_path": ".env"
    }
  ],
  "manual_call_templates": [],
  "post_processing": [],
  "tool_repository": {
    "tool_repository_type": "in_memory",
    "tools": [
      {
        "name": "go.build",
        "description": "Build Go packages",
        "tags": ["go", "build"],
        "inputs": {
          "packages": { "type": "string", "default": "./..." },
          "output": { "type": "string", "default": "" }
        },
        "outputs": { "output_path": { "type": "string" } },
        "tool_call_template": {
          "call_template_type": "cli",
          "commands": [ "go build -o UTCP_ARG_output_UTCP_END UTCP_ARG_packages_UTCP_END" ]
        }
      },
      {
        "name": "go.test",
        "description": "Test Go packages",
        "tags": ["go", "test"],
        "inputs": {
          "packages": { "type": "string", "default": "./..." },
          "flags": { "type": "string" }
        },
        "outputs": { "result": { "type": "string" } },
        "tool_call_template": {
          "call_template_type": "cli",
          "commands": [ "go test UTCP_ARG_packages_UTCP_END UTCP_ARG_flags_UTCP_END" ]
        }
      },
      {
        "name": "go.test_coverage",
        "description": "Test Go packages with coverage",
        "tags": ["go", "test", "coverage"],
        "inputs": {
          "packages": { "type": "string", "default": "./..." },
          "format": { "type": "string", "default": "html" }
        },
        "outputs": { "coverage_file": { "type": "string" } },
        "tool_call_template": {
          "call_template_type": "cli",
          "commands": [ "go test -coverprofile=coverage.out UTCP_ARG_packages_UTCP_END && go tool cover -UTCP_ARG_format_UTCP_END=coverage.html -o coverage.html coverage.out" ]
        }
      },
      {
        "name": "go.mod_tidy",
        "description": "Tidy Go module dependencies",
        "tags": ["go", "mod"],
        "inputs": {},
        "outputs": { "output": { "type": "string" } },
        "tool_call_template": {
          "call_template_type": "cli",
          "commands": [ "go mod tidy" ]
        }
      },
      {
        "name": "go.lint",
        "description": "Lint Go code with vet",
        "tags": ["go", "lint"],
        "inputs": {
          "packages": { "type": "string", "default": "./..." }
        },
        "outputs": { "result": { "type": "string" } },
        "tool_call_template": {
          "call_template_type": "cli",
          "commands": [ "go vet UTCP_ARG_packages_UTCP_END" ]
        }
      },
      {
        "name": "go.fmt",
        "description": "Format Go code",
        "tags": ["go", "format"],
        "inputs": {
          "paths": { "type": "string", "default": "." }
        },
        "outputs": { "result": { "type": "string" } },
        "tool_call_template": {
          "call_template_type": "cli",
          "commands": [ "go fmt UTCP_ARG_paths_UTCP_END" ]
        }
      },
      {
        "name": "go.run",
        "description": "Run Go program",
        "tags": ["go", "run"],
        "inputs": {
          "main": { "type": "string", "default": "./cmd" },
          "args": { "type": "array", "items": { "type": "string" } }
        },
        "outputs": { "stdout": { "type": "string" } },
        "tool_call_template": {
          "call_template_type": "cli",
          "commands": [ "go run UTCP_ARG_main_UTCP_END UTCP_ARG_args_UTCP_END" ]
        }
      },
      {
        "name": "marklogic.search",
        "description": "Query MarkLogic search",
        "tags": ["marklogic", "search"],
        "inputs": {
          "query": { "type": "object" },
          "options": { "type": "object" }
        },
        "outputs": { "results": { "type": "object" } },
        "tool_call_template": {
          "call_template_type": "http",
          "http_method": "POST",
          "url": "{{MARKLOGIC_BASE_URL}}/v1/search",
          "auth": {
            "auth_type": "basic",
            "username": "{{MARKLOGIC_USERNAME}}",
            "password": "{{MARKLOGIC_PASSWORD}}"
          },
          "body": {
            "query": "{{query}}",
            "options": "{{options}}"
          }
        }
      },
      {
        "name": "marklogic.getDocument",
        "description": "Get MarkLogic document",
        "tags": ["marklogic", "document"],
        "inputs": {
          "uri": { "type": "string" }
        },
        "outputs": { "document": { "type": "object" } },
        "tool_call_template": {
          "call_template_type": "http",
          "http_method": "GET",
          "url": "{{MARKLOGIC_BASE_URL}}/v1/documents?uri={{uri}}",
          "auth": {
            "auth_type": "basic",
            "username": "{{MARKLOGIC_USERNAME}}",
            "password": "{{MARKLOGIC_PASSWORD}}"
          }
        }
      },
      {
        "name": "marklogic.putDocument",
        "description": "Create/update MarkLogic document",
        "tags": ["marklogic", "document", "write"],
        "inputs": {
          "uri": { "type": "string" },
          "content": { "type": "object" },
          "format": { "type": "string", "default": "json" }
        },
        "outputs": { "status": { "type": "string" } },
        "tool_call_template": {
          "call_template_type": "http",
          "http_method": "PUT",
          "url": "{{MARKLOGIC_BASE_URL}}/v1/documents?uri={{uri}}",
          "auth": {
            "auth_type": "basic",
            "username": "{{MARKLOGIC_USERNAME}}",
            "password": "{{MARKLOGIC_PASSWORD}}"
          },
          "headers": {
            "Content-Type": "application/{{format}}"
          },
          "body": "{{content}}"
        }
      }
    ]
  },
  "tool_search_strategy": {
    "tool_search_strategy_type": "tag_and_description_word_match"
  }
}
